w data-event-opts="{{[['tap',[['getBack',['$event']]]]]}}" class="boxStyle" bindtap="__e"><image style="width:100%;height:100%;border-radius:26rpx;" src="{{userlicenseBack}}" name="userlicenseBack" mode="aspectFill"></image></view></block><text class="fontStyle">驾照副页</text><view class="topClass"><text class="titleClass">{{title}}</text><image class="returnClass" src="../../static/grzx/btnReturn.png" data-event-opts="{{[['tap',[['returnClick',['$event']]]]]}}" bindtap="__e"></image><image class="navClass" src="../../static/grzx/nav3.png"></image></view><view data-event-opts="{{[['tap',[['submitClick',['$event']]]]]}}" class="submitClass" bindtap="__e">提交</view></view>