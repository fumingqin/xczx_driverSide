<view class="content"><text class="titleClass">{{title}}</text><image class="returnClass" src="../../static/grzx/btnReturn.png" data-event-opts="{{[['tap',[['returnClick',['$event']]]]]}}" bindtap="__e"></image><image class="navClass" src="../../static/grzx/nav2.png"></image><view class="boxClass"><image class="portraitClass" src="{{portrait||'/static/grzx/touxiang.png'}}"></image><text data-event-opts="{{[['tap',[['getPhoto',['$event']]]]]}}" class="photoGraph" bindtap="__e">{{photoGraph}}</text><view class="boxClass1 m-t b-b"><text class="fontClass">姓名</text><input class="inputClass" name="userName" placeholder="请输入真实姓名" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" bindinput="__e"/></view><view class="boxClass1 b-b"><text class="fontClass">性别</text><radio-group class="sexClass" name="userSex"><block wx:for="{{sexMode}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label data-event-opts="{{[['tap',[['radioClick',[index]]]]]}}" bindtap="__e"><radio style="transform:scale(0.7);" value="{{userSex}}" checked="{{index===userSex}}" color="#FC4646"></radio>{{item.title+''}}</label></block></radio-group></view><view class="boxClass1 b-b"><text class="fontClass">公司</text><picker class="companyClass" name="userCompany" mode="selector" range="{{companyBox}}" value="{{userCompany}}" data-event-opts="{{[['change',[['companyChange',['$event']]]]]}}" bindchange="__e">{{''+userCompany+''}}</picker></view><view class="boxClass1 b-b"><text class="fontClass">驾照等级</text><text data-event-opts="{{[['tap',[['openPopup',['bottomPopup']]]]]}}" class="licenseClass" bindtap="__e">{{selector}}</text></view><view data-event-opts="{{[['tap',[['nextClick',['$event']]]]]}}" class="nextClass" bindtap="__e">下一步</view></view><uni-popup class="vue-ref" vue-id="113b57b3-1" type="bottom" data-ref="bottomPopup" bind:__l="__l" vue-slots="{{['default']}}"><view class="popupBox" style="display:flex;flex-direction:column;"><view class="topClass"><view data-event-opts="{{[['tap',[['closePopup',['bottomPopup']]]]]}}" class="closeClass" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['submitClick',['$event']]]]]}}" class="submitClass" bindtap="__e">确定</view></view><scroll-view class="listClass" scroll-y="true"><checkbox-group data-event-opts="{{[['change',[['checkboxChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="__i0__" wx:key="value"><label class="labelClass"><view style="display:flex;flex-direction:row;"><checkbox value="{{item.value}}" checked="{{item.checked}}"></checkbox><view>{{item.name}}</view></view></label></block></checkbox-group></scroll-view></view></uni-popup></view>